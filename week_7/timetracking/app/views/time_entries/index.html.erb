<h1><%= @project.name %></h1>
<ul>
  <% @entries.each do |entry| %>
    <li>
      [<%= link_to entry.id, edit_project_time_entry_path(@project, entry) %>]
      <%= entry.hours %> h
      <%= entry.minutes %> m -
      <%= formatted_date(entry.date) %>
      :: <%= entry.comments %>
    </li>
  <% end %>
</ul>
